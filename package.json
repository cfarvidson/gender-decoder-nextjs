{
  "name": "gender-decoder-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "pnpm lang && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "lang:extract": "NODE_ENV=development lingui extract --clean",
    "lang:compile": "lingui compile",
    "lang": "pnpm lang:extract && pnpm lang:compile",
    "prepare": "husky",
    "detect-unused": "node scripts/detect-unused.js",
    "detect-circular-imports": "madge --circular src/",
    "all": "pnpm build && jest && pnpm detect-unused && pnpm detect-circular-imports"
  },
  "dependencies": {
    "@lingui/core": "5.1.0",
    "@lingui/react": "5.1.0",
    "classnames": "^2.5.1",
    "is-hotkey": "^0.2.0",
    "lodash": "^4.17.21",
    "make-plural": "^7.4.0",
    "next": "15.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@lingui/cli": "5.1.0",
    "@lingui/loader": "5.1.0",
    "@lingui/macro": "5.1.0",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/is-hotkey": "^0.1.10",
    "@types/jest": "29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/node": "22.10.1",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "autoprefixer": "^10.4.20",
    "babel-cli": "^6.26.0",
    "babel-jest": "29.7.0",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "9.16.0",
    "eslint-config-next": "15.0.4",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "madge": "8.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "typescript": "5.7.2"
  }
}
